{
  "cli": {
    "version": ">= 16.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      
      "env": {
        "EXPO_DOCTOR_SKIP_DEPENDENCY_VERSION_CHECK": "1",
        "APP_VARIANT": "development"
      },
      "environment": "development"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview"
      },
      "environment": "preview"
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "APP_VARIANT": "production"
      },
      "environment": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
