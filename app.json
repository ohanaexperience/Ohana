{
  "expo": {
    "name": "Ohana",
    "slug": "ohanaapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ohanaapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ohanaexperiences.expoapp",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleTypeRole": "Editor",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1036045272087-rj7bfqar6lhv4u0jc4vg8ni0uoa8i606",
              "fb8762220613880930"
            ]
          }
        ],
        "NSCameraUsageDescription": "We use your camera so you can upload a profile photo.",
        "NSPhotoLibraryUsageDescription": "We use your photo library to let you upload profile photos."
      }
    },
    "android": {
      "package": "com.ohanaexperiences.expoapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "com.ohanaexperiences.expoapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "podfileProperties": {
              "use_modular_headers!": true
            }
          }
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.yourid",
          "enableGooglePay": true
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.1036045272087-rj7bfqar6lhv4u0jc4vg8ni0uoa8i606"
        }
      ],
      [
        "react-native-fbsdk-next",
        {
          "appID": "8762220613880930",
          "clientToken": "934e68962fa8a9e616117867e2c84b6e",
          "displayName": "Ohana",
          "scheme": "fb8762220613880930",
          "advertiserIDCollectionEnabled": false,
          "autoLogAppEventsEnabled": false,
          "isAutoInitEnabled": true,
          "iosUserTrackingPermission": "Used to deliver personalized ads."
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "f30962b9-4591-4f81-84a8-8d16193c94e0"
      },
      "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "1036045272087-m020u7sbmf6p4r08cikj61ufs2u6sl58.apps.googleusercontent.com",
      "GOOGLE_IOS_CLIENT_ID": "1036045272087-rj7bfqar6lhv4u0jc4vg8ni0uoa8i606.apps.googleusercontent.com",
      "GOOGLE_MAPS_API_KEY": "${GOOGLE_MAPS_API_KEY}"
    },
    "owner": "ohanaorg"
  }
}
