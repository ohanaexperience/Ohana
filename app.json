{
  "expo": {
    "name": "Ohana",
    "slug": "ohanaapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ohanaapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ohanaexperiences.expoapp",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleTypeRole": "Editor",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1036045272087-e1l55rh7afnnj8lnsb4bkumue8kd3p2a",
              "fb8762220613880930"
            ]
          }
        ],
        "NSCameraUsageDescription": "This app needs access to your camera to allow you to upload a profile photo.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload a profile photo."
      }
    },
    "android": {
      "package": "com.ohanaexperiences.expoapp",
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "com.ohanaexperiences.expoapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.yourid",
          "enableGooglePay": true
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.1036045272087-e1l55rh7afnnj8lnsb4bkumue8kd3p2a"
        }
      ],
      [
        "react-native-fbsdk-next",
        {
          "appID": "8762220613880930",
          "clientToken": "934e68962fa8a9e616117867e2c84b6e",
          "displayName": "Ohana",
          "scheme": "fb8762220613880930",
          "advertiserIDCollectionEnabled": false,
          "autoLogAppEventsEnabled": false,
          "isAutoInitEnabled": true,
          "iosUserTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f30962b9-4591-4f81-84a8-8d16193c94e0"
      },
      "GOOGLE_MAPS_API_KEY": "${GOOGLE_MAPS_API_KEY}",
      "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "${EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID}"
    },
    "owner": "ohanaorg"
  }
}
